#include <assert.h>
#include <stdio.h>
#include <stdlib.h>

typedef enum { MATERIAL_AIR } Material;

typedef struct {
  float pressure;
  Material material;
} Voxel;

typedef struct {
  size_t dim_x, dim_y, dim_z;
  Voxel (*data)[3][dim_x][dim_y][dim_z];
} World;

void world_alloc(size_t x, size_t y, size_t z, Voxel (**world)[3][x][y][z]) {

  *world = malloc(sizeof(Voxel[3][x][y][z]));
  assert(*world != NULL);
}

void world_fill(size_t x, size_t y, size_t z, Voxel (**world)[3][x][y][z], )

    int main() {
  printf("Hello, World!\n");
}
